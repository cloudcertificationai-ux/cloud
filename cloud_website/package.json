{
  "name": "cloud-certification",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "NODE_OPTIONS='--max-old-space-size=4096' next dev",
    "build": "NODE_OPTIONS='--max-old-space-size=4096' next build",
    "start": "next start",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "type-check": "tsc --noEmit --project tsconfig.build.json",
    "postinstall": "prisma generate",
    "postbuild": "next-sitemap",
    "analyze": "ANALYZE=true npm run build",
    "analyze:server": "BUNDLE_ANALYZE=server npm run build",
    "analyze:browser": "BUNDLE_ANALYZE=browser npm run build",
    "analyze:bundle": "node scripts/analyze-bundle.js analyze",
    "analyze:size": "node scripts/analyze-bundle.js size",
    "build:production": "NODE_ENV=production npm run build",
    "deploy:preview": "vercel",
    "deploy:production": "vercel --prod",
    "deploy:check": "vercel inspect",
    "precommit": "npm run lint && npm run type-check && npm run test",
    "prebuild": "npm run type-check",
    "health-check": "curl -f http://localhost:3000/api/health || exit 1",
    "test:comprehensive": "./scripts/test-comprehensive.sh",
    "test:comprehensive:analyze": "./scripts/test-comprehensive.sh --analyze",
    "validate:seo": "node scripts/validate-seo.js",
    "test:seo": "npm run test:e2e tests/e2e/final-seo-validation.spec.ts",
    "validate:seo:full": "npm run validate:seo && npm run test:seo",
    "db:seed": "tsx prisma/seed.ts",
    "db:reset": "prisma migrate reset && npm run db:seed",
    "db:reset:dev": "tsx scripts/reset-database.ts",
    "aggregate-stats": "tsx src/scripts/aggregate-daily-stats.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.990.0",
    "@aws-sdk/s3-request-presigner": "^3.990.0",
    "@headlessui/react": "^2.2.9",
    "@heroicons/react": "^2.2.0",
    "@mdx-js/loader": "^3.1.1",
    "@mdx-js/react": "^3.1.1",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@next/mdx": "^16.1.1",
    "@prisma/adapter-pg": "^7.3.0",
    "@prisma/client": "^7.4.0",
    "@stripe/stripe-js": "^8.7.0",
    "@types/mdx": "^2.0.13",
    "@vercel/analytics": "^1.6.1",
    "@vercel/speed-insights": "^1.3.1",
    "bullmq": "^5.69.2",
    "clsx": "^2.1.1",
    "critters": "^0.0.23",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.3",
    "hls.js": "^1.6.15",
    "ioredis": "^5.9.2",
    "next": "16.1.1",
    "next-auth": "^4.24.13",
    "pg": "^8.17.2",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "redis": "^5.10.0",
    "stripe": "^20.3.0",
    "tailwind-merge": "^3.4.0",
    "web-vitals": "^5.1.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^20",
    "@types/pg": "^8.16.0",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/uuid": "^10.0.0",
    "babel-plugin-react-compiler": "1.0.0",
    "eslint": "^9",
    "eslint-config-next": "16.1.1",
    "fast-check": "^4.5.2",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "next-sitemap": "^4.2.3",
    "prettier": "^3.7.4",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "prisma": "^7.4.0",
    "tailwindcss": "^4",
    "tsx": "^4.21.0",
    "typescript": "^5",
    "webpack-bundle-analyzer": "^5.1.0"
  }
}
